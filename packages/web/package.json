	"scripts": {
		"locale:sort": "eslint src/Locale/**/*.json --fix && prettier --write src/Locale/**/*.json",
		"start": "dotenv-extended --path=.env.local --defaults=../../.env.defaults react-app-rewired start",
		"proxy": "dotenv-extended --path=.env.local --defaults=../../.env.defaults node ./scripts/socket-proxy.js",
		"build": "dotenv-extended --path=.env.production --defaults=../../.env.defaults react-app-rewired build",
		"test": "dotenv-extended --path=.env.jest --defaults=../../.env.defaults react-app-rewired test --watchAll=false",
		"lint": "eslint src src/Locale/**/*.json --ext .ts,.tsx --max-warnings 0",
		"lint:fix": "eslint src src/Locale/**/*.json --ext .ts,.tsx --fix || echo",
		"prettier": "prettier --check src src/Locale",
		"prettier:fix": "prettier --write src src/Locale",
		"lint:strict": "yarn lint-staged --diff dev...HEAD --quiet",
		"eject": "react-app-rewired eject",
		"stream": "ffmpeg -re -f lavfi -i testsrc -c:v libx264 -b:v 1600k -preset ultrafast -b 900k -c:a libfdk_aac -b:a 128k -s 1920x1080 -x264opts keyint=50 -g 25 -pix_fmt yuv420p -f flv ",
		"deploy": "dotenv-extended --path=.env.production --defaults=../../.env.defaults node ./scripts/deployment.js",
		"storybook": "DISABLE_ESLINT_PLUGIN=true storybook dev -p 6006 -s ../public",
		"build-storybook": "storybook build -s public",
		"publish-storybook": "yarn chromatic",
		"serve": "serve -s build"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"ie 11",
